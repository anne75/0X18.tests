#!/bin/bash
#
# Tests for identical output to intranet example for project 15: stack, queue
tmp_file="checker_tmp_file_$RANDOM"

# clean up
stop_monty

# run command
$MONTY pstr/samples/pstr_empty_input > $OUTPUTFILE 2> /dev/null &

# wait a little bit
$SLEEP $SLEEPSECONDS

# check the result
diff $OUTPUTFILE pstr/samples/pstr_empty_output > /dev/null
if [ $? -eq 0 ]; then
      print_ok
else
	print_ko
fi

# clean up
stop_monty

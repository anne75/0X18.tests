#!/bin/bash
#
# Tests for identical output to intranet example for project 15: stack, queue
tmp_file="checker_tmp_file_$RANDOM"

# clean up
stop_monty

# run command
$MONTY ./stack_queue/samples/intranet_example_input > $OUTPUTFILE 2> /dev/null &

# wait a little bit
$SLEEP $SLEEPSECONDS

# check the result
nmatch=`diff $OUTPUTFILE ./stack_queue/samples/intranet_example_output/`
if [ $1 -eq 0 ]; then
      print_ok
else
	print_ko
fi

# clean up
stop_monty

# Configuration file

TESTDIR="."
R=$RANDOM
OUTPUTFILE="checker_output_$R"
ERROROUTPUTFILE="checker_error_output_$R"
MONTY="./monty"

SLEEP=`which sleep`
SLEEPSECONDS=0.5
LTRACE=`which ltrace`
LTRACEOUTPUTFILE="checker_ltrace_output_$R"
ECHO=`which echo`
CAT=`which cat`
GREP=`which grep`
WC=`which wc`
RM=`which rm`
TOUCH=`which touch`
CHMOD=`which chmod`
DIFF=`which diff`
